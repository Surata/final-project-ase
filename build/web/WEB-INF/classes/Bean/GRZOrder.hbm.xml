<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="Bean.GRZOrder" table="order">
    <id name="orderID" column="ID" type="integer">
        <generator class="increment"></generator>
    </id>
    <property name="cartID" column="CartID" type="integer" />
    <property name="productID" column="ProductID" type="integer" />
    <property name="quantity" column="Quantity" type="integer" />
    <property name="subTotal" column="SubTotal" type="float" />
    <property name="date" column="Date" type="timestamp" />
  </class>
</hibernate-mapping>
