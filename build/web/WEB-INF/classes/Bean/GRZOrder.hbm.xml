<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="Bean.GRZOrder" table="orders">
    <id column="ID" name="orderID" type="integer">
        <generator class="increment"></generator>
    </id>
    <property name="transactionID" column="TransactionID" type="integer"></property>
    <property name="productID" column="ProductID" type="integer"></property>
    <property name="quantity" column="Quantity" type="integer"></property>
    <property name="subTotal" column="SubTotal" type="float"></property>
  </class>
</hibernate-mapping>
