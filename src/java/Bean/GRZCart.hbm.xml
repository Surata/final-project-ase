<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="Bean.GRZCart" table="cart">
      <id name="cartID" column="ID" type="integer">
          <generator class="increment"></generator>
      </id>
      <property name="userID" column="UserID" type="integer" />
      <property name="status" column="Status" type="integer" />
      <property name="total" column="Total" type="float" />
      <property name="date" column="Date" type="string" />
  </class>
</hibernate-mapping>
